<% if (typeof success !== 'undefined' && success && success.length > 0) { %>
    <div class="alert alert-success alert-dismissible fade show col-6 offset-3" role="alert"><%= success[0] %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close"></button>
    </div>
<% } %>

<% if (typeof error !== 'undefined' && error && error.length > 0) { %>
    <div class="alert alert-danger alert-dismissible fade show col-6 offset-3" role="alert"><%= error[0] %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close"></button>
    </div>
<% } %>
